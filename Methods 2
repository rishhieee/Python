a="Hi this is Rishitha"
print(a.lower())
print(a.upper())
print(a.capitalize())
print(a.split())
print(a.title())
print(a.swapcase())
b="/"
print(b.join (a))
print(a.center(3,"/"))
print(a.replace("s","t"))
print(a.count("universe"))
print(a.isupper())
print(a.islower())
print(a.isalpha())
print(a.isnumeric())
print(a.isalnum())
print(a.isspace())
print(a.istitle())
print(a.startswith("H"))
print(a.endswith("E"))
print(a.find("there"))
print(len(a))
print(min(a), max(a))

