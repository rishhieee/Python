rows = 3
columns = 5

matrix = [[0 for i in range(columns)] for j in range(rows)]
matrix[0][0] = 1
matrix[0][1] = 2
matrix[0][2] = 3
matrix[0][3]=90
matrix[0][4]=87
matrix[1][0] = 4
matrix[1][1] = 5
matrix[1][2] = 6
matrix[1][3]=2
matrix[1][4]=23
matrix[2][0] = 7
matrix[2][1] = 8
matrix[2][2] = 9
matrix[2][3]=3
matrix[2][4]=7
for row in matrix:
    for element in row:
        print(element, end=" ")
    print()
